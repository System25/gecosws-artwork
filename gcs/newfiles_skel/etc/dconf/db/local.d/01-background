# Set GECOS background image
[org/gnome/desktop/background]
picture-uri='file:///usr/share/backgrounds/gecos/gecos_panoramico.jpg'
picture-options='scaled'
primary-color='000000'

[org/cinnamon/desktop/background]
picture-uri='file:///usr/share/backgrounds/gecos/gecos_panoramico.jpg'
primary-color='000000'
picture-options='zoom'

[org/cinnamon/desktop/background/slideshow]
image-source='xml:///usr/share/cinnamon-background-properties/gecos.xml'
delay=15


